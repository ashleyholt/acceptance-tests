{
  "name": "Single-token admin areas analysis should search for venues",
  "endpoint": "search",
  "priorityThresh": 10,
  "distanceThresh": 50,
  "tests": [
    {
      "id": 1,
      "user": "trescube",
      "status": "pass",
      "description": "there should be nothing named New York near Socorro, NM",
      "in": {
        "text": "new york",
        "focus.point.lat": "34.060746",
        "focus.point.lon": "-106.900002"
      },
      "expected": {
        "properties": [
          {
            "layer": "locality"
          }
        ]
      },
      "unexpected": {
        "properties": [
          {
            "layer": "venue"
          }
        ]
      }
    },
    {
